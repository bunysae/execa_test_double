{
	"name": "execa_test_double",
	"version": "0.0.1-0",
	"description": "Test doubles for process execution with execa",
	"license": "MIT",
	"repository": "bunysae/execa_test_double",
	"files": [
		"index.js",
		"src"
	],
	"scripts": {
		"test": "xo && FORCE_HYPERLINK=1 ava"
	},
	"keywords": [
		"execa",
		"mock",
		"test double"
	],
	"dependencies": {
		"globby": "^9.0.0",
		"sinon": "^9.0.0"
	},
	"devDependencies": {
		"ava": "^3.0.0",
		"xo": "*"
	},
	"xo": {
		"rules": {
			"ava/no-import-test-files": [
				"off",
				{
					"files": [
						"test/*"
					]
				}
			]
		}
	}
}
